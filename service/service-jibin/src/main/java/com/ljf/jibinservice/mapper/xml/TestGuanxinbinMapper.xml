<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ljf.jibinservice.mapper.TestGuanxinbinMapper">

    <!--查询每个年龄有多少行-->
    <select id="countNumOfEveryAge" resultType="java.lang.Integer">
        SELECT count(*) FROM test_guanxinbin gxb
        WHERE uc.age=#{strPerAge}
    </select>
</mapper>
